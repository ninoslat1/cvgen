import { getLocaleFromLang } from "@/utils/getLocale";
export async function getStaticPaths() {
  return [
    { params: { lang: "en" } },
    { params: { lang: "id" } },
  ];
}

const { lang } = Astro.params;
const locale = await getLocaleFromLang(lang);

if(locale){
    return Astro.redirect(`/${locale}/`);
} else {
    return Astro.redirect(``)
}
